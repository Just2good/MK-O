<?xml version="1.0" encoding="utf-8"?>
	
<ms2>

	<state name="대기">
		<condition name="유저를감지했으면" arg1="915" arg2="">
			<transition state="작동"/>	
		</condition>
	</state>

	<state name="작동">
		<onEnter>
			<action name="액터를설정한다" arg1="9901" arg2="1" arg3="Closed" />
			<action name="버프를걸어준다" arg1="915" arg2="70002104" arg3="1" arg4="" arg5="0" />
		</onEnter>
		<condition name="유저를감지했으면" arg1="911" arg2="">
			<transition state="감지"/>	
		</condition>
		<condition name="UserValue" key="GravityRoom" value="1" >
			<transition state="종료"/>	
		</condition>
	</state>
	
	<state name="감지">
		<onEnter>
			<action name="액터를설정한다" arg1="9901" arg2="1" arg3="Opened" />
			<action name="버프를걸어준다" arg1="915" arg2="70002103" arg3="1" arg4="" arg5="0" />
		</onEnter>
		<condition name="UserValue" key="GravityRoom" value="1" >
			<transition state="종료"/>	
		</condition>
		<condition name="!유저를감지했으면" arg1="911" arg2="">
			<transition state="작동"/>	
		</condition>
	</state>

	<state name="종료">
		<onEnter>
			<action name="액터를설정한다" arg1="9901" arg2="0" arg3="Opened" />
		</onEnter>
	</state>

</ms2>

